		"responses": {},
		"schemas": {
			"UserStatus": {
				"enum": [
					"active",
					"blocked"
				],
				"type": "string"
			},
			"ProfileDto": {
				"properties": {
					"id": {
						"type": "number",
						"format": "double"
					},
					"email": {
						"type": "string"
					},
					"userId": {
						"type": "number",
						"format": "double"
					},
					"firstName": {
						"type": "string"
					},
					"middleName": {
						"type": "string"
					},
					"lastName": {
						"type": "string"
					},
					"birthday": {
						"type": "string",
						"format": "date-time"
					},
					"status": {
						"$ref": "#/components/schemas/UserStatus"
					},
					"createdAt": {
						"type": "string",
						"format": "date-time"
					},
					"updatedAt": {
						"type": "string",
						"format": "date-time"
					}
				},
				"required": [
					"id",
					"email",
					"userId",
					"firstName",
					"middleName",
					"lastName",
					"birthday",
					"status",
					"createdAt"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "profile",
		"version": "1.0.0",
		"description": "profile service",
